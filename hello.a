	exit = 1
	%ex_code = %r0
	ex_success = 0
	write = 4
	%wr_file = %r0
	stdout = 1
	%wr_buffer = %r1
	%wr_len = %r2

	%trap = %r7

	text
	%trap <- write
	%wr_file <- stdout
	%wr_buffer <- %pc + (msg - . - 8)
	%wr_len <- msg_len
	trap 0

	%trap <- exit
	%ex_code <- ex_success
	trap 0

msg:
	bytes "Hallo Welt\n"
msg_len = . - msg
